import java.math.*;

public class line extends point
{
	float xielv;
	String color;
	float xiankuan;
	float changdu;
	
	
	void line(int a[],int b[],String color,float xiankuan)
	{
		this.xielv=(a[1]-b[1])/(a[0]-b[0]);
		this.changdu=(float) Math.sqrt((a[0]-b[0])*(a[0]-b[0])+(a[1]-b[1])*(a[1]-b[1]));
		this.color=color;
		this.xiankuan=xiankuan;	
	}
	
	public static void main(String[] args)
	{
		line L = null;
		String r="red";
		float k=1;
		int a[]= {0,1};
		int b[]= {1,2};
		
		L.line(a,b,r,k);
	}

	

}